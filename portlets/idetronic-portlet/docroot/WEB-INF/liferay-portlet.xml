<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>usx</portlet-name>
		<ajaxable>true</ajaxable> 
		<add-default-resource>false</add-default-resource> 
		<system>true</system>
	</portlet>
	
	<portlet>
		<portlet-name>exp</portlet-name>
		<ajaxable>true</ajaxable> 
		<header-portlet-css>/css/bootstrap-tagsinput.css</header-portlet-css>
		<footer-portlet-javascript>/js/jquery-1.11.0.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/bootstrap.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/bootstrap-tagsinput.min.js</footer-portlet-javascript>
		<add-default-resource>false</add-default-resource> 
		<system>true</system>
		
	</portlet>
	<portlet>
		<portlet-name>org</portlet-name>
		<ajaxable>true</ajaxable> 
		<header-portlet-css>/css/orgchart/custom.css</header-portlet-css>
		<header-portlet-css>/css/orgchart/jquery.jOrgChart.css</header-portlet-css>
		<footer-portlet-javascript>/js/org/jquery-1.7.1.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/org/jquery-ui.1.7.1.min.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/org/jquery.jOrgChart.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/org/taffy.js</footer-portlet-javascript>
		<add-default-resource>false</add-default-resource> 
		<system>true</system>
		
	</portlet>
	<portlet>
		<portlet-name>userdisplay</portlet-name>
		<icon>/icon.png</icon>
		<header-portlet-css>/css/directory/main.css</header-portlet-css>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>usxdisplay</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>