<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.idetronic.ruangj.model.impl.ItemModelImpl">
		<attributes>
			<id name="itemId" />
			<basic name="companyId" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="status" />
			<basic name="itemType" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.idetronic.ruangj.model.impl.ItemFileEntryModelImpl">
		<attributes>
			<id name="itemEntryId" />
			<basic name="itemId" />
			<basic name="fileEntryId" />
			<basic name="fileType" />
			<basic name="userId" />
			<basic name="userName" />
			<basic name="createDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.idetronic.ruangj.model.impl.ItemImpl" name="Item">
		<table name="ruangj_Item" />
		<attributes />
	</entity>
	<entity class="com.idetronic.ruangj.model.impl.ItemFileEntryImpl" name="ItemFileEntry">
		<table name="ruangj_ItemFileEntry" />
		<attributes />
	</entity>
</entity-mappings>