<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_0_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.1.2+</version>
	</compatibility>
	<theme id="km-bootstrap" name="Km Bootstrap">
	<color-scheme id="01" name="Grey">
	        <css-class>grey</css-class>
	        <color-scheme-images-path>
            	${images-path}/color_schemes/${css-class}
        	</color-scheme-images-path>
        </color-scheme>
		<color-scheme id="02" name="Orange">
	        <css-class>ora</css-class>
	        <color-scheme-images-path>
            	${images-path}/color_schemes/${css-class}
        	</color-scheme-images-path>
        </color-scheme>
		<color-scheme id="03" name="Blue">
	        <css-class>blue</css-class>
	        <color-scheme-images-path>
            	${images-path}/color_schemes/${css-class}
        	</color-scheme-images-path>
        </color-scheme>
		<color-scheme id="04" name="Purple">
	        <css-class>purple</css-class>
	        <color-scheme-images-path>
            	${images-path}/color_schemes/${css-class}
        	</color-scheme-images-path>
        </color-scheme>
	</theme>
</look-and-feel>